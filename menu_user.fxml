<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #3A6B35;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.banking_system_fx.Menu_User_Controller">
   <children>
      <TitledPane animated="false" layoutX="199.0" layoutY="200.0" style="-fx-background-color: #E3B448;" text="Check balance">
        <content>
          <AnchorPane fx:id="balance" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
         <font>
            <Font name="Roboto" size="12.0" />
         </font>
      </TitledPane>
      <Button fx:id="deposit" layoutX="70.0" layoutY="116.0" mnemonicParsing="false" onAction="#choose_deposit" style="-fx-background-color: #E3B448;" text="Deposit">
         <font>
            <Font name="Roboto" size="12.0" />
         </font></Button>
      <Button fx:id="withdraw" layoutX="487.0" layoutY="119.0" mnemonicParsing="false" onAction="#choose_withdraw" style="-fx-background-color: #E3B448;" text="Withdraw">
         <font>
            <Font name="Roboto" size="12.0" />
         </font></Button>
      <Text fill="#cbd18f" layoutX="223.0" layoutY="76.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-background-color: #CBD18F;" text="Choose action">
         <font>
            <Font name="Roboto Bold" size="24.0" />
         </font>
      </Text>
      <Button fx:id="back" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#go_back" style="-fx-background-color: #CBD18F;" text="&lt;--">
         <font>
            <Font name="Roboto" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
